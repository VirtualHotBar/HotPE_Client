{
  "compilerOptions": {
    "target": "esNext",
    "module": "commonjs",    
    "outDir": "build-mian",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
    },
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./main/**/*"
  ]
}